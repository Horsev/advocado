{
  "extends": ["airbnb", "prettier", "plugin:node/recommended"],
  "plugins": ["prettier"],
  "rules": {
    "no-use-before-define": "off",
    "no-unused-expressions": "off",
    "prettier/prettier": "error",
    "no-console": "error",
    "no-unused-vars": "warn",
    "func-names": "warn",
    "no-process-exit": "warn",
    "object-shorthand": "warn",
    "class-methods-use-this": "warn"
  },  "env": {
    "browser": true,
    "node": true
  }
}